<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.adminpage">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".ActivityOrder"></activity>
        <activity android:name=".ActivityCategory"></activity>
        <activity android:name=".ActivityUser"></activity>
        <activity android:name=".ActivityCategoryList"></activity>
        <activity android:name=".ActivityCategoryAdd"></activity>
        <activity android:name=".ActivityCategoryListPhone"></activity>
        <activity android:name=".ActivityCategoryListLaptop"></activity>
        <activity android:name=".ActivityCategoryListAcce"></activity>
        <activity android:name=".ActivityCategoryAddProduct"></activity>
        <activity android:name=".ActivityCategoryEditProduct"></activity>
        <activity android:name=".ActivityCategoryEditUser"></activity>
        <activity android:name=".ActivityListOrderDetails"></activity>


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>